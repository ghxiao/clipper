<?xml version="1.0"?>
<rdf:RDF xmlns="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#"
     xml:base="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://creativecommons.org/ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration">
        <owl:versionIRI rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration-1.0"/>
        <owl:imports rdf:resource="http://www.opengis.net/ont/geosparql"/>
        <dc:contributor>Hallstein Lie</dc:contributor>
        <dc:description>The ontology describes parts of the petroleum subsurface exploration domain including the classes for and properties about fields, wells, and wellbores.</dc:description>
        <dc:modified>2017-05-01</dc:modified>
        <rdfs:label>Subsurface Exploration Ontology</rdfs:label>
        <dc:contributor>Valerio Santarelli</dc:contributor>
        <vann:preferredNamespaceUri>http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration</vann:preferredNamespaceUri>
        <dc:creator>Dag Hovland</dc:creator>
        <ns:licence rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <dc:title>Subsurface Exploration Ontology</dc:title>
        <owl:versionInfo>1.0</owl:versionInfo>
        <dc:contributor>Roman Kontchakov</dc:contributor>
        <dc:issued>2017-05-01</dc:issued>
        <dc:creator>Martin G. Skj√¶veland</dc:creator>
        <dc:contributor>Domenico Fabio Savo</dc:contributor>
        <dc:contributor>Toralv Nordtveit</dc:contributor>
        <vann:preferredNamespacePrefix>expl</vann:preferredNamespacePrefix>
        <dc:description rdf:resource="http://vowl.visualdataweb.de/webvowl/#iri=http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration"/>
        <rdfs:comment>The ontology describes parts of the petroleum subsurface exploration domain including the classes for and properties about fields, wells, and wellbores.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#commment -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#commment"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#length -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#length"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#activityEnd -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#activityEnd">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Activity"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#activityParticipant -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#activityParticipant">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Activity"/>
        <rdfs:comment>Something that has a duration, it exists between a start and an end.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#activityStart -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#activityStart">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Activity"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#baseSurface -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#baseSurface">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicUnit"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicSurface"/>
        <rdfs:comment>Relates a stratigraphic unit to the base surface (also called exit surface).</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#concededBy -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#concededBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Concession"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#AdministrativeEntity"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#drilledByRig -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#drilledByRig">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#activityParticipant"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#RigOperation"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#DrillingRig"/>
        <rdfs:comment>Relates a rig operation to the rig that does/did the operation. Usually there is only one rig involved, but it can occur that several are involved, e.g. in case of malfunction or interrupted operation. </rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#drilledInterval -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#drilledInterval">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreDrilling"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <rdfs:comment>The interval of the wellbore that was drilled during one registered session of drilling. Probably this is a section of the wellbore. This connects the time of the drilling with the depth.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#drillingTDReached -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#drillingTDReached">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#DrillingOperation"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#DrillingTDReached"/>
        <rdfs:comment>Related a drilling operation and the event of reaching terminal depth, that is reaching the maximum depth. This is an important milestone in a drilling operation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#extractedFrom -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#extractedFrom">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Core"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <rdfs:comment>The wellbore interval that a core was extracted from. Since the core has physically been removed from the wellbore, we do not identify these two objects, although it is common to do so.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasActivityPart -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasActivityPart">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasBottomDepth -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasBottomDepth">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasDepth"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <rdfs:comment>The base of a wellbore interval. The bottom is the point of the wellbore interval with the highest measured depth. Note that this may have a lower true vertical depth than other parts of the interval, since wellbore can bend.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasConvergence -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasConvergence">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasCoreSample -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasCoreSample">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Core"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#CoreSample"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasDatumElevation -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasDatumElevation">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasDepth -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasDepth">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Depth"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasEastOffset -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasEastOffset">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellborePoint"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Length"/>
        <rdfs:comment>The eastward distance from the well to a point on the wellbore</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasElevation -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasElevation">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#RigOperation"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Depth"/>
        <rdfs:comment>The depth reference, elevation, during this operation.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasFormationPressure -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasFormationPressure">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#FormationPressure"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasGeochemicalMeasurement -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasGeochemicalMeasurement">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#GeochemicalMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasLogCurve -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasLogCurve">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#LogCurve"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement">
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Measurement"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasNorthOffset -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasNorthOffset">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellborePoint"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Length"/>
        <rdfs:comment>The northward distance from the well reference point to a point along the wellbore path.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasPermeability -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasPermeability">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#CoreSample"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Permeability"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasPlatform -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasPlatform">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#SurfaceFacility"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasPorosity -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasPorosity">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#CoreSample"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Porosity"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasSlotEastWest -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasSlotEastWest">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasSlotNorthSouth -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasSlotNorthSouth">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasThickness -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasThickness">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Thickness"/>
        <rdfs:comment>Gets the thickness of an object. In this ontology, used for the various measures of thickness of stratigraphic zones.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasTopDepth -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasTopDepth">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasDepth"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <rdfs:comment>Gets the depth of the top of a wellbore interval. Note that the top means the point closest to the surface going along the wellbore, that is, the point with the lowest measured depth. Since wellbores may bend, this point may actually be farther from the surface in direct vertical distance.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasTotalCoreLength -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasTotalCoreLength">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#TotalCoreLength"/>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasUnit -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasUnit">
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicZone"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicUnit"/>
        <rdfs:comment>The stratigraphic unit that a zone is found to belong to. A zone is a concrete part/interval of a wellbore, while a unit is a more abstract type of rock, for example Jurassic.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasWaterDepth -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasWaterDepth">
        <rdfs:subPropertyOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasMeasurement"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WaterDepth"/>
        <rdfs:comment>The average depth of the sea at which the well lies. Note that this is the result of an average taken over time, not a single measurement.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasWellbore -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasWellbore">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well"/>
        <rdfs:range rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
        <rdfs:comment>Relates a well to the wellbores starting at the well. This is everywhere assumed to be inverse functional, e.g. by the hierarchical national naming schemes. However, some new techniques allow entering an existing wellbore under the ground, hence creating entry to the same wellbore from different wells. </rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasWellboreInterval -->

    <owl:ObjectProperty rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasWellboreInterval">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
