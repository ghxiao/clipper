        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicUnit"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellborePoint"/>
        <owl:disjointWith rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:comment>The occurrence of a stratigraphic surface in a wellbore.</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicSurface -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicSurface">
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicUnit"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <owl:disjointWith rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:comment>Upper or lower boundary of a stratigraphic unit.</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicUnit -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicUnit">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#ofStratigraphicColumn"/>
                <owl:someValuesFrom rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicColumn"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#name"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellborePoint"/>
        <owl:disjointWith rdf:resource="http://www.opengis.net/ont/geosparql#Geometry"/>
        <rdfs:comment>A type of rock or layer in the crust of the earth. Can be classified after different hierarchies called stratigraphic columns. For example, Jurassic is a chronostratigraphic unit, indicating rock from the Jurassic period.</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicZone -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicZone">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasUnit"/>
                <owl:someValuesFrom rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#StratigraphicUnit"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment>A depth interval at which the wellbore is deemed to intersect some stratigraphic unit. Note that a stratigraphic unit may be intersected several times by the same wellbore.</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#SubSeaTemplate -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#SubSeaTemplate">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#ProductionFacility"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#SurfaceFacility"/>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#SurfaceFacility -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#SurfaceFacility">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#ProductionFacility"/>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Temperature -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Temperature">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Measurement"/>
        <rdfs:comment>The temperature at a given point and time</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Thickness -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Thickness">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Length"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasThickness"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WellboreInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#TotalCoreLength"/>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WaterDepth"/>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#TotalCoreLength -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#TotalCoreLength">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Length"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty>
                    <rdf:Description>
                        <owl:inverseOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#hasTotalCoreLength"/>
                    </rdf:Description>
                </owl:onProperty>
                <owl:someValuesFrom rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Wellbore"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WaterDepth"/>
        <rdfs:comment>The sum of the lengths of all the cores extracted from a wellbore</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#TrueVerticalDepth -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#TrueVerticalDepth">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Depth"/>
        <rdfs:comment>The direct vertical depth from sea bottom or horizon to a given point in a wellbore.</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WaterDepth -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#WaterDepth">
        <rdfs:subClassOf rdf:resource="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Length"/>
        <rdfs:comment>The average depth of water at some point</rdfs:comment>
    </owl:Class>
    


    <!-- http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well -->

    <owl:Class rdf:about="http://slegger.gitlab.io/slegge-obda/ontology/subsurface-exploration#Well">
