    d2rq:uriPattern "authors/@@dblp_authorid_ref_new.author|urlify@@";
    d2rq:class foaf:Agent; # sometimes also organizations are authors
    d2rq:dataStorage map:DB;
#    d2rq:containsDuplicates "true";
    .

