        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The description of how to produce a design specification.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/Systems.owl#functionality-specification-workflow -->

    <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/Systems.owl#functionality-specification-workflow">
