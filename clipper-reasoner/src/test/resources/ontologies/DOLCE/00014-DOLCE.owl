    <owl:NamedIndividual rdf:about="http://www.loa-cnr.it/ontologies/SemioticCommunicationTheory.owl#channel-role">
        <rdf:type rdf:resource="http://www.loa-cnr.it/ontologies/SemioticCommunicationTheory.owl#communication-role"/>
        <rdf:type>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#played-by"/>
