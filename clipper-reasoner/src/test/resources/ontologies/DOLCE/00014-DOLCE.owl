        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The task sequencing an activity from which the possibility is raised to execute a plan.</rdfs:comment>
    </owl:NamedIndividual>
    


    <!-- http://www.loa-cnr.it/ontologies/Plans.owl#preparedness-task -->

    <owl:NamedIndividual rdf:about="http://www.loa-cnr.it/ontologies/Plans.owl#preparedness-task">
        <rdf:type rdf:resource="http://www.loa-cnr.it/ontologies/Plans.owl#plan-assessment-task"/>
