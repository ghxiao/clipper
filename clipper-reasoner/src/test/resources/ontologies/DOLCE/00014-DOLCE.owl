                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.loa-cnr.it/ontologies/Plans.owl#action-task"/>
                                    <rdf:Description rdf:about="http://www.loa-cnr.it/ontologies/Plans.owl#complex-task"/>
                                </owl:unionOf>
                            </owl:Class>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#direct-predecessor"/>
                                <owl:someValuesFrom>
