        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The description of how to implement a system-as-artifact.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/Systems.owl#system-production-workflow -->

    <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/Systems.owl#system-production-workflow">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.loa-cnr.it/ontologies/Systems.owl#workflow"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#specifically-constantly-dependent-on"/>
                        <owl:someValuesFrom rdf:resource="http://www.loa-cnr.it/ontologies/Systems.owl#system-design"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#specifically-constantly-dependent-on"/>
                        <owl:someValuesFrom rdf:resource="http://www.loa-cnr.it/ontologies/Systems.owl#system-functionality"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The description of how a system is produced.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.loa-cnr.it/ontologies/Systems.owl#workflow -->

    <owl:Class rdf:about="http://www.loa-cnr.it/ontologies/Systems.owl#workflow">
        <rdfs:subClassOf rdf:resource="http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#plan"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#d-uses"/>
