        </rdf:type>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The channel role in Jakobson&#39;s theory of communication.</rdfs:comment>
        <ExtendedDnS:defined-by rdf:resource="http://www.loa-cnr.it/ontologies/SemioticCommunicationTheory.owl#s-communication-theory"/>
    </owl:NamedIndividual>
    


    <!-- http://www.loa-cnr.it/ontologies/SemioticCommunicationTheory.owl#code-role -->

    <owl:NamedIndividual rdf:about="http://www.loa-cnr.it/ontologies/SemioticCommunicationTheory.owl#code-role">
        <rdf:type rdf:resource="http://www.loa-cnr.it/ontologies/ExtendedDnS.owl#description-role"/>
        <rdf:type rdf:resource="http://www.loa-cnr.it/ontologies/SemioticCommunicationTheory.owl#communication-role"/>
