    </owl:NamedIndividual>
    


    <!-- http://www.loa-cnr.it/ontologies/Plans.owl#synchro-task -->

    <owl:NamedIndividual rdf:about="http://www.loa-cnr.it/ontologies/Plans.owl#synchro-task">
        <rdf:type rdf:resource="http://www.loa-cnr.it/ontologies/Plans.owl#control-task"/>
