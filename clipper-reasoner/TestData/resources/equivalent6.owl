<?xml version="1.0" encoding="ISO-8859-1"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xml:base="http://www.example.org/ontology"
    xmlns="http://www.example.org/ontology#">

<owl:Ontology rdf:about="">
  <rdfs:comment>An example OWL ontology</rdfs:comment>
</owl:Ontology>

<!-- Equivalence. This one is DL -->

<owl:Class rdf:about="#A">
  <owl:equivalentClass rdf:nodeID="X"/>
  <owl:equivalentClass rdf:nodeID="Y"/>
</owl:Class>

<owl:Restriction rdf:nodeID="X">
  <owl:onProperty>
    <owl:ObjectProperty rdf:about="#p"/>
  </owl:onProperty>
  <owl:minCardinality>1</owl:minCardinality>
  <owl:equivalentClass rdf:nodeID="Y"/>
</owl:Restriction>

<owl:Restriction rdf:nodeID="Y">
  <owl:onProperty>
    <owl:ObjectProperty rdf:about="#r"/>
  </owl:onProperty>
  <owl:minCardinality>1</owl:minCardinality>
</owl:Restriction>

</rdf:RDF>
