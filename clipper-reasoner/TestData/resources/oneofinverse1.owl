<?xml version="1.0" encoding="ISO-8859-1"?>
<rdf:RDF xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xml:base="http://www.example.org/ontology"
    xmlns="http://www.example.org/ontology#">

<owl:Class rdf:about="#NiceCorporation">
 <rdfs:subClassOf>
  <owl:Restriction>
   <owl:onProperty rdf:resource="#employs"/>
   <owl:someValuesFrom>
    <owl:Class>
     <owl:oneOf rdf:parseType="Collection">
      <owl:Thing rdf:about="#dick"/>
      <owl:Thing rdf:about="#tom"/>
     </owl:oneOf>
    </owl:Class>
   </owl:someValuesFrom>
  </owl:Restriction>
 </rdfs:subClassOf>
 <rdfs:subClassOf>
  <owl:Restriction>
   <owl:onProperty rdf:resource="#employs"/>
   <owl:allValuesFrom>
    <owl:Class rdf:about="#NiceGuy"/>
   </owl:allValuesFrom>
  </owl:Restriction>
 </rdfs:subClassOf>
</owl:Class>

<owl:Class rdf:about="#NiceGuy"/>

<owl:ObjectProperty rdf:about="#employs"/>

<owl:ObjectProperty rdf:about="#employedBy">
 <owl:inverseOf rdf:resource="#employs"/>
</owl:ObjectProperty>

<NiceCorporation rdf:about="#niceCorp"/>

<rdf:Description rdf:about="#dick">
 <rdf:type>
  <owl:Class>
   <owl:complementOf>
    <owl:Class rdf:about="#NiceGuy"/>
   </owl:complementOf>
  </owl:Class>
 </rdf:type>
</rdf:Description>

<rdf:Description rdf:about="#tom">
 <rdf:type>
  <owl:Restriction>
   <owl:onProperty rdf:resource="#employedBy"/>
   <owl:allValuesFrom>
    <owl:Class>
     <owl:complementOf>
      <owl:Class rdf:about="#NiceCorporation"/>
     </owl:complementOf>
    </owl:Class>
   </owl:allValuesFrom>
  </owl:Restriction>
 </rdf:type>
</rdf:Description>

</rdf:RDF>
