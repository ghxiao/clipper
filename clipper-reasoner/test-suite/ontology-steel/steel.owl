    <owl:ObjectProperty rdf:about="http://ontorule-project.eu/resources/steel#hasSamplingPoint">
        <rdfs:label xml:lang="en">has sampling point</rdfs:label>
        <rdfs:domain rdf:resource="http://ontorule-project.eu/resources/steel#Coil"/>
        <rdfs:range rdf:resource="http://ontorule-project.eu/resources/steel#SamplingPoint"/>
        <owl:inverseOf rdf:resource="http://ontorule-project.eu/resources/steel#inCoil"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://ontorule-project.eu/resources/steel#inCoil -->

    <owl:ObjectProperty rdf:about="http://ontorule-project.eu/resources/steel#inCoil">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">in coil</rdfs:label>
        <rdfs:comment>This property links sampling points, used by the system to measure some physical parameters such as the yield strength, to the product (a given coil).</rdfs:comment>
        <rdfs:range rdf:resource="http://ontorule-project.eu/resources/steel#Coil"/>
        <rdfs:domain rdf:resource="http://ontorule-project.eu/resources/steel#SamplingPoint"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


