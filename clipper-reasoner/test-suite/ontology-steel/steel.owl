        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://ontorule-project.eu/resources/steel#derivesFrom -->

    <owl:ObjectProperty rdf:about="http://ontorule-project.eu/resources/steel#derivesFrom">
        <rdfs:label xml:lang="en">derives from</rdfs:label>
