    <!-- http://ontorule-project.eu/resources/steel#action -->

    <owl:ObjectProperty rdf:about="http://ontorule-project.eu/resources/steel#action">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">action</rdfs:label>
        <rdfs:comment>This is the final assignment of a coil to an order (a customer)</rdfs:comment>
