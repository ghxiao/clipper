        <rdfs:comment>A defect is detected comparing coil data with order specifications in sampling points. Thus a defect is relative to a position in the coil.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ontorule-project.eu/resources/steel#Defect"/>
        <rdfs:range rdf:resource="http://ontorule-project.eu/resources/steel#SamplingPoint"/>
        <rdfs:subPropertyOf rdf:resource="&owl;topObjectProperty"/>
    </owl:ObjectProperty>
    


